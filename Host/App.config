<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
	</startup>
	<system.serviceModel>
		<behaviors>
			<serviceBehaviors>
				<behavior name="mexBehavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service behaviorConfiguration="mexBehavior" name="ServiciosCalabozosDragones.ImplementacionServicio">
				<endpoint address="ServiciosCalabozosDragones" binding="netTcpBinding" contract="ServiciosCalabozosDragones.IGestionCuenta" />
				<endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8080/ServiciosCalabozosDragones/" />
						<add baseAddress="net.tcp://localhost:8090" />
					</baseAddresses>
				</host>
			</service>
			<service name="Host.Servicio">
				<endpoint address="" binding="basicHttpBinding" contract="Host.IServicio">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
			</service>
		</services>


	</system.serviceModel>
</configuration>
